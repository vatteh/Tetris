{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "rules": {
    "arrow-parens": [0],
    "max-len": [1, 120, 2, {ignoreComments: true}],
    "prop-types": [0],
    "no-unused-vars": [0],
    "no-param-reassign": ["error", { "props": false }],
    "no-plusplus": [0],
    "no-mixed-operators": [0]
  },
  "extends": ["airbnb-base"],
  "env": {
      "browser": true,
      "node": true
  }
}
